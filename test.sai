var max = list::[Int] => {
    match list {
    | []     -> None
    | (x:xs) ->
         current = max xs in 
        (match current {
        | None -> x
        | ?    ->
            case {
            | x > current  -> x
            | x <= current -> current
            }
        })
    }
}

