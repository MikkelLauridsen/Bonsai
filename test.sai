var max = list::[43.3, 32.2] => {
    match list {
    | []     -> None
    | (x:xs) ->
    let current = max xs in 
        (match current {
        | None -> x
        | ?    ->
            case {
            | x > current  -> x
            | x <= current -> current
            }
        })
    }
}

